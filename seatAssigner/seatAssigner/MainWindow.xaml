<Window x:Class="seatAssigner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:seatAssigner"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="seatAssignBut" Content="Assign Seats" HorizontalAlignment="Left" Height="43" Margin="10,169,0,0" VerticalAlignment="Top" Width="132" Click="Button_Click"/>
        <Button x:Name="nameReadBut" Content="Read Names" HorizontalAlignment="Left" VerticalAlignment="Top" Width="132" Margin="10,108,0,0" Height="28" Click="nameReadBut_Click"/>
        <TextBox x:Name="fileReadTXT" HorizontalAlignment="Left" Height="33" Margin="10,47,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="132"/>
        <ListBox x:Name="assignedSeatsLB" HorizontalAlignment="Left" Height="326" Margin="182,10,0,0" VerticalAlignment="Top" Width="546"/>

    </Grid>
</Window>
